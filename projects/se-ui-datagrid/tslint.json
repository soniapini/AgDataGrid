{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "se",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "se",
      "kebab-case"
    ],
    "import-spacing": true,
    "indent": [
      true,
      "spaces"
    ],
    "interface-over-type-literal": true
  }
}
